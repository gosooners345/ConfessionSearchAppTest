<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.confessionsearchapptest.release1.ui.home.SearchFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_notes"
        android:name="com.confessionsearchapptest.release1.ui.NotesActivity.NotesFragment"
        android:label="@string/notes"
        tools:layout="@layout/fragment_notes" />
    <fragment
        android:id="@+id/navigation_bible"
        android:name="com.confessionsearchapptest.release1.ui.bible.BibleFragment"
        android:label="Bible Reader" />
    <fragment
        android:id="@+id/navigation_help"
        android:name="com.confessionsearchapptest.release1.ui.help.HelpPageFragment"
        tools:layout="@layout/fragment_help_page" />

</navigation>